#!/bin/bash
{{ range .packages.rpm-tree -}}
sudo rpm-ostree install {{ . | quote }}
{{ end -}}
{{ range .packages.pip -}}
pip install -- user {{ . | quote }}
{{ end -}}
{{ range .packages.cargo -}}
cargo install {{ . | quote }}
{{ end -}}
{{ range .packages.fisher -}}
fisher install {{ . | quote }}
{{ end -}}
